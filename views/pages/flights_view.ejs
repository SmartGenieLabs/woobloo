      <!-- Header -->
      <div
        class="header pb-8 pt-5 pt-lg-8 d-flex align-items-center"
        style="min-height: 600px; background-image: url(/img/people/flight.jpg); background-size: cover; background-position: center top;"
      >
        <!-- Mask -->
        <span class="mask bg-gradient-default opacity-8"></span>
        <!-- Header container -->
        <div class="container-fluid d-flex align-items-center">
          <div class="row">
            <div class="col-lg-7 col-md-10">
              <h1 class="display-2 text-white">Hello Bob Wysocki,</h1>
              <p class="text-white mt-0 mb-5">
                This is flights Dashboard. Here you can view the requests
                associated with the flight intent which is a part of woobloo
                assistant.
              </p>
              <div class="row">
                <div class="col-xl-6 col-lg-9">
                  <div class="card card-stats mb-4 mb-xl-0">
                    <a href="/flights_view">
                      <div class="card-body">
                        <div class="row">
                          <div class="col">
                            <h5
                              class="card-title text-uppercase text-muted mb-0"
                            >
                              View
                            </h5>
                            <span class="h2 font-weight-bold mb-0"
                              >Requests</span
                            >
                          </div>
                          <div class="col-auto">
                            <div
                              class="icon icon-shape bg-danger text-white rounded-circle shadow"
                            >
                              <i class="ni ni-curved-next"></i>
                            </div>
                          </div>
                        </div>
                      </div>
                    </a>
                  </div>
                </div>
                <div class="col-xl-6 col-lg-9">
                  <div class="card card-stats mb-4 mb-xl-0">
                    <a href="/flights_edit">
                      <div class="card-body">
                        <div class="row">
                          <div class="col">
                            <h5
                              class="card-title text-uppercase text-muted mb-0"
                            >
                              Edit
                            </h5>
                            <span class="h2 font-weight-bold mb-0"
                              >Response</span
                            >
                          </div>
                          <div class="col-auto">
                            <div
                              class="icon icon-shape bg-warning text-white rounded-circle shadow"
                            >
                              <i class="ni ni-chat-round"></i>
                            </div>
                          </div>
                        </div>
                      </div>
                    </a>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
      <!-- Page content -->
      <div class="container-fluid mt--7">
        <div class="row">
          <div class="col">
            <div class="card shadow">
              <div class="card-header border-0">
                <h3 class="mb-0">
                  <i class="ni ni-align-left-2 text-yellow"></i>&nbsp View
                  Requests
                </h3>
              </div>
              <div class="table-responsive">
                <table class="table align-items-center table-flush">
                  <thead class="thead-light">
                    <tr>
                      <th scope="col"><h5>Username</h5></th>
                      <th scope="col"><h5>Arrival</h5></th>
                      <th scope="col"><h5>Destination</h5></th>
                      <th scope="col"><h5>Status</h5></th>
                      <th scope="col"></th>
                    </tr>
                  </thead>
                  <tbody>
                    <tr>
                      <th scope="row">
                        <div class="media align-items-center">
                          <a href="#" class="avatar rounded-circle mr-3">
                            <img
                              alt="Image placeholder"
                              src="/img/people/siri.jpg"
                            />
                          </a>
                          <div class="media-body">
                            <span class="mb-0 text-sm">Sirisha Gondi</span>
                          </div>
                        </div>
                      </th>
                      <td>Bangalore</td>
                      <td>Chennai</td>
                      <td>
                        <span class="badge badge-dot mr-4">
                          <i class="bg-warning"></i> pending
                        </span>
                      </td>
                      <td class="text-right">
                        <div class="dropdown">
                          <a
                            class="btn btn-sm btn-icon-only text-light"
                            href="#"
                            role="button"
                            data-toggle="dropdown"
                            aria-haspopup="true"
                            aria-expanded="false"
                          >
                            <i class="fas fa-ellipsis-v"></i>
                          </a>
                          <div
                            class="dropdown-menu dropdown-menu-right dropdown-menu-arrow"
                          >
                            <a class="dropdown-item" href="#">pending</a>
                            <a class="dropdown-item" href="#">completed</a>
                            <a class="dropdown-item" href="#">delayed</a>
                            <a class="dropdown-item" href="#">on schedule</a>
                          </div>
                        </div>
                      </td>
                    </tr>
                    <tr>
                      <th scope="row">
                        <div class="media align-items-center">
                          <a href="#" class="avatar rounded-circle mr-3">
                            <img
                              alt="Image placeholder"
                              src="/img/people/sandy.png"
                            />
                          </a>
                          <div class="media-body">
                            <span class="mb-0 text-sm">Sandeep Gondi</span>
                          </div>
                        </div>
                      </th>
                      <td>Chennai</td>
                      <td>Delhi</td>
                      <td>
                        <span class="badge badge-dot mr-4">
                          <i class="bg-info"></i> on schedule
                        </span>
                      </td>
                      <td class="text-right">
                        <div class="dropdown">
                          <a
                            class="btn btn-sm btn-icon-only text-light"
                            href="#"
                            role="button"
                            data-toggle="dropdown"
                            aria-haspopup="true"
                            aria-expanded="false"
                          >
                            <i class="fas fa-ellipsis-v"></i>
                          </a>
                          <div
                            class="dropdown-menu dropdown-menu-right dropdown-menu-arrow"
                          >
                            <a class="dropdown-item" href="#">pending</a>
                            <a class="dropdown-item" href="#">completed</a>
                            <a class="dropdown-item" href="#">delayed</a>
                            <a class="dropdown-item" href="#">on schedule</a>
                          </div>
                        </div>
                      </td>
                    </tr>
                    <tr>
                      <th scope="row">
                        <div class="media align-items-center">
                          <a href="#" class="avatar rounded-circle mr-3">
                            <img
                              alt="Image placeholder"
                              src="/img/people/bob.png"
                            />
                          </a>
                          <div class="media-body">
                            <span class="mb-0 text-sm">Bob Wysocki</span>
                          </div>
                        </div>
                      </th>
                      <td>Mumbai</td>
                      <td>Chennai</td>
                      <td>
                        <span class="badge badge-dot mr-4">
                          <i class="bg-danger"></i> delayed
                        </span>
                      </td>
                      <td class="text-right">
                        <div class="dropdown">
                          <a
                            class="btn btn-sm btn-icon-only text-light"
                            href="#"
                            role="button"
                            data-toggle="dropdown"
                            aria-haspopup="true"
                            aria-expanded="false"
                          >
                            <i class="fas fa-ellipsis-v"></i>
                          </a>
                          <div
                            class="dropdown-menu dropdown-menu-right dropdown-menu-arrow"
                          >
                            <a class="dropdown-item" href="#">pending</a>
                            <a class="dropdown-item" href="#">completed</a>
                            <a class="dropdown-item" href="#">delayed</a>
                            <a class="dropdown-item" href="#">on schedule</a>
                          </div>
                        </div>
                      </td>
                    </tr>
                    <tr>
                      <th scope="row">
                        <div class="media align-items-center">
                          <a href="#" class="avatar rounded-circle mr-3">
                            <img
                              alt="Image placeholder"
                              src="/img/people/shash.png"
                            />
                          </a>
                          <div class="media-body">
                            <span class="mb-0 text-sm">Shashwat Bishwen</span>
                          </div>
                        </div>
                      </th>
                      <td>Jaipur</td>
                      <td>Indore</td>
                      <td>
                        <span class="badge badge-dot mr-4">
                          <i class="bg-success"></i> completed
                        </span>
                      </td>
                      <td class="text-right">
                        <div class="dropdown">
                          <a
                            class="btn btn-sm btn-icon-only text-light"
                            href="#"
                            role="button"
                            data-toggle="dropdown"
                            aria-haspopup="true"
                            aria-expanded="false"
                          >
                            <i class="fas fa-ellipsis-v"></i>
                          </a>
                          <div
                            class="dropdown-menu dropdown-menu-right dropdown-menu-arrow"
                          >
                            <a class="dropdown-item" href="#">pending</a>
                            <a class="dropdown-item" href="#">completed</a>
                            <a class="dropdown-item" href="#">delayed</a>
                            <a class="dropdown-item" href="#">on schedule</a>
                          </div>
                        </div>
                      </td>
                    </tr>
                    <% for(var i = 0; i < result.length; i++) { %>
                      <tr>
                          <th scope="row">
                            <div class="media align-items-center">
                              <a href="#" class="avatar rounded-circle mr-3">
                                <img
                                  alt="Image placeholder"
                                  src="/img/people/shash.png"
                                />
                              </a>
                              <div class="media-body">
                                <span class="mb-0 text-sm"><%= result[i].username %></span>
                              </div>
                            </div>
                          </th>
                          <td><%= result[i].arrival %></td>
                          <td><%= result[i].destination %></td>
                          <td>
                            <span class="badge badge-dot mr-4">
                              <i class="bg-warning"></i> <%= result[i].status %>
                            </span>
                          </td>
                          <td class="text-right">
                            <div class="dropdown">
                              <a
                                class="btn btn-sm btn-icon-only text-light"
                                href="#"
                                role="button"
                                data-toggle="dropdown"
                                aria-haspopup="true"
                                aria-expanded="false"
                              >
                                <i class="fas fa-ellipsis-v"></i>
                              </a>
                              <div
                                class="dropdown-menu dropdown-menu-right dropdown-menu-arrow"
                              >
                                <a class="dropdown-item" href="#">pending</a>
                                <a class="dropdown-item" href="#">completed</a>
                                <a class="dropdown-item" href="#">delayed</a>
                                <a class="dropdown-item" href="#">on schedule</a>
                              </div>
                            </div>
                          </td>
                        </tr>
                     <% } %>
                  </tbody>
                </table>
              </div>
              <div class="card-footer py-4">
                <nav aria-label="...">
                  <ul class="pagination justify-content-end mb-0">
                    <li class="page-item disabled">
                      <a class="page-link" href="#" tabindex="-1">
                        <i class="fas fa-angle-left"></i>
                        <span class="sr-only">Previous</span>
                      </a>
                    </li>
                    <li class="page-item active">
                      <a class="page-link" href="#">1</a>
                    </li>
                    <li class="page-item">
                      <a class="page-link" href="#"
                        >2 <span class="sr-only">(current)</span></a
                      >
                    </li>
                    <li class="page-item">
                      <a class="page-link" href="#">3</a>
                    </li>
                    <li class="page-item">
                      <a class="page-link" href="#">
                        <i class="fas fa-angle-right"></i>
                        <span class="sr-only">Next</span>
                      </a>
                    </li>
                  </ul>
                </nav>
              </div>
            </div>
          </div>
        </div>
        <!-- Footer -->
        <footer class="footer">
          <div class="row align-items-center justify-content-xl-between">
            <div class="col-xl-6">
              <div class="copyright text-center text-xl-left text-muted">
                &copy; 2018 | All rights reserved.
              </div>
            </div>
            <div class="col-xl-6">
              <ul
                class="nav nav-footer justify-content-center justify-content-xl-end"
              >
                <li class="nav-item">
                  <h6 class="nav-link" target="_blank">
                    Made with <i class="ni ni-favourite-28 text-red"></i> by<a
                      href="https://www.woobloo.com"
                      class="font-weight-bold ml-1"
                      target="_blank"
                      >Woobloo</a
                    >
                    for a better society.
                  </h6>
                </li>
              </ul>
            </div>
          </div>
        </footer>
      </div>
    </div>
    <!-- Scripts -->

    <script src="/others/jquery/dist/jquery.min.js"></script>
    <script src="/others/bootstrap/dist/js/bootstrap.bundle.min.js"></script>

    <script src="/js/woobloo.js"></script>
  </body>
